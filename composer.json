{
    "name": "rollerworks/cache-bundle",
    "description": "Symfony integration bundle for Rollerworks Session Cache",
    "keywords": ["cache", "symfony", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3|~3.0",
        "rollerworks/cache": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Bundle\\CacheBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Bundle\\CacheBundle\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
